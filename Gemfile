source "https://rubygems.org"

gem "jekyll", "~> 4.3" # Jekyllのバージョンを指定。
# gem "github-pages", group: :jekyll_plugins # この行をコメントアウトまたは削除します

# 必要なJekyllプラグインは個別に記述します。
# あなたの _config.yml に jekyll-feed と jekyll-sitemap があるので、これらを追加します。
gem "jekyll-feed"
gem "jekyll-sitemap"

group :jekyll_plugins do
  # その他のプラグインをここに追加できます
  # gem "jekyll-archives"
end